<% if @user.errors.any? %>
  <% @user.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $("#new_user").hide();
  //$("#registration-div").html("<%= escape_javascript(render 'shared/user_registration/team_form') %>")
<% end %>